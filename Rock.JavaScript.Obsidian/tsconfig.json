{
  "compilerOptions": {
    "baseUrl": "./",
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noEmitOnError": true,
    "removeComments": true,
    "allowUnreachableCode": false,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "target": "ES2016",
    "moduleResolution": "Node",
    "lib": [
      "DOM",
      "ES2016",
      "ES2020.Symbol.WellKnown" /* Fixes compile issue in Vue declaration files, not actually in use. */
    ],
  }
}
